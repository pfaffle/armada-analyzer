{
  "name": "@pfaffle/armada-analyzer-service",
  "version": "0.0.1",
  "type": "module",
  "description": "Analyzes the data from STFC armadas",
  "scripts": {
    "build": "pnpm run compile && pnpm run copy:static",
    "clean": "rm -rf dist",
    "compile": "tsc -p ./tsconfig.build.json",
    "copy:static": "tsx scripts/copyStatic.ts",
    "lint": "eslint .",
    "start:server": "node dist/bin/start.js",
    "start": "pnpm run build && pnpm run start:server",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfaffle/armada-analyzer.git"
  },
  "author": "Craig Meinschein <cmeinsch@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pfaffle/armada-analyzer/issues"
  },
  "homepage": "https://github.com/pfaffle/armada-analyzer#readme",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^5.0.1",
    "morgan": "~1.9.1"
  }
}
